@charset "utf-8";
/* CSS Document */
*{
	margin:0px;
	padding:0px;
	font-family:"微软雅黑","黑体";
	font-size:14px;
	list-style:none;
}
a{
	text-decoration:none;
	outline:none;
}
img{
	border:0px;
	vertical-align:middle;	
	}
table{
	border-collapse:collapse;
}
button{cursor:pointer;}
.common{
	text-align:center;
}
.common_1{
	cursor:pointer;
}
.common_font{
	font-size:14px;
}
.common_color{
	color:#FFF;
}
/*-设置成灰色-*/
.common_color2{color:#989898;}
.common_bgcolor{
 background:#355A86;
}/*设置主色调的蓝颜色*/
.common_float_left{
	float:left;
}
.common_float_right{
	float:right;
}
.common_margin{
	margin:auto;}
/*pc:包含在iframe中的div*/
.content{
	width:1135px;
	margin:auto;
	min-height:820px;
	position:relative;
}
/*pc*/
.gray-trabecula{
	background-color:#dbdad6;
	border:solid thin #e3e2df;
	height:33px;
	width:1119px;
}
/*pc*/
.gray-trabecula span{
	color:#535252;
	float:left;
	line-height:33px;
}
/*pc*/
.gray-trabecula h4{
	color:#333333;
	font-weight:lighter;
}
/*pc*/
.trabecula h2{
	color:#8C8C8C;
	font-size:24px;
	font-weight:lighter;
	line-height:48px;
}
.trabecula font{
	font-size:16px;
}
/*pc*/
.trabecula-white{
	padding-left:5px;
	height:48px;
}
<html>
<head>
<title>新建生产线</title>
</head>
  <body>
    <div style="width:438px; height:410px;">
    <form action="" method="post">
    <div class="box">
     <table style="width:380px;height:395px;">
          <!--生产线类型-->
        <tr>
            <td>生产线类型：</td>
            <td align="left">
             <div class="selected_div">
              <select name="product_line_type" >
                  <option>1</option>
                  <option>2</option>
                  <option>3</option>
               </select>
               </div>
      
            </td>
        </tr>
        <!--安装周期-->
        <tr>
          <td>安装周期：</td>
          <td align="left">
             <input type="text" name="install_cycle">
          </td>
        </tr>
        <!--每期暗安装费用-->
        <tr>
            <td>每期暗安装费用：</td>
            <td align="left">
               <input type="text" name="install_cost">
            </td>
        </tr>
        
          <!--转产周期-->
        <tr>
            <td>转产周期：</td>
            <td align="left">
              <input type="text" name="change_cycle">
            </td>
        </tr>
        
          <!--每期转产费用-->
        <tr>
            <td>每期转产费用：</td>
            <td align="left">
              <input type="text" name="change_cost">
            </td>
        </tr>
        
          <!--生产效率-->
        <tr>
            <td>生产效率：</td>
            <td align="left">
              <input type="text" name="efficient">
            </td>
        </tr>
        
        
          <!--每期维护费用-->
        <tr>
            <td>每期维护费用：</td>
            <td align="left">
              <input type="text" name="maintenance_cost">
            </td>
        </tr>
        
          <!--每期折旧费用-->
        <tr>
            <td>每期折旧费用：</td>
            <td align="left">
              <input type="text" name="depreciation_cost">
            </td>
        </tr>
        
          <!--残值数额-->
        <tr>
            <td>残值数额：</td>
            <td align="left">
              <input type="text" name="salvage value">
            </td>
        </tr>
        
        <!--确认新建-->
        <tr><td colspan="2" align="center" ><input type="submit" name="submit" class="sub" value="确认新建"/></td></tr>
     
     </table>  
    
    </div>
    <!--box结束-->
    </form>
    </div>
  </body>
</html>